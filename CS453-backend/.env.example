# Gemini API Configuration
# Get your API key from https://ai.google.dev/tutorials/setup
# NOTE: This is for reference only. You must also configure Continue.dev at ~/.continue/config.yaml
# with your Gemini API key for the 'cn' command to work.
GEMINI_API_KEY=your_gemini_api_key_here

# Session Configuration
SESSION_SECRET=your_session_secret_here

# GitHub OAuth (for authentication)
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# N8N Webhook URL (for audio transcription)
N8N_WEBHOOK_URL=http://localhost:5678/webhook-test/ec52a91a-54e0-47a2-afa3-f191c87c7043
